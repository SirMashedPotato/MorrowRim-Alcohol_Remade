<Defs>

  <ThingDef ParentName="ResourceBase">
    <defName>MorrowRim_DagothBrandyPrecursor</defName>
    <label>dagoth brandy precursor</label>
    <description>Un-fermented dagoth brandy. This substance needs to ferment in a fermenting barrel before it becomes drinkable dagoth brandy.</description>
    <graphicData>
      <texPath>Things/Item/Resource/MorrowRim_DagothBrandyPrecursor</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <healthAffectsPrice>false</healthAffectsPrice>
    <rotatable>false</rotatable>
    <stackLimit>75</stackLimit>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <statBases>
      <DeteriorationRate>1.0</DeteriorationRate>
      <MarketValue>2.5</MarketValue>
      <Mass>0.8</Mass>
      <Flammability>0.2</Flammability>
      <MaxHitPoints>60</MaxHitPoints>
    </statBases>
    <tickerType>Rare</tickerType>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>5</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>
  
