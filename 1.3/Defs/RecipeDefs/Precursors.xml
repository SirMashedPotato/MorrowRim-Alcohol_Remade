<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>MorrowRim_MakeDagothBrandyPrecursor</defName>
    <label>make dagoth brandy precursor</label>
    <description>Make dagoth brandy precursor from comberry and chemfuel.</description>
    <jobString>Making dagoth brandy precursor.</jobString>
    <workSpeedStat>DrugCookingSpeed</workSpeedStat>
    <workSkill>Cooking</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>1000</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MorrowRim_RawComberry</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MorrowRim_RawComberry</li>
        <li>Chemfuel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <MorrowRim_DagothBrandyPrecursor>5</MorrowRim_DagothBrandyPrecursor>
    </products>
	<researchPrerequisite>MorrowRim_BoozeMaking</researchPrerequisite>
  </RecipeDef>
  
